<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GifMoviewView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifMoviewViewStyle" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="asia.ivity.android.marqueeview.MarqueeView">
        <attr name="speed" format="integer"/>
        <attr name="pause" format="integer"/>
        <attr name="autoStart" format="boolean"/>
    </declare-styleable>
    
    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>
   
</resources>